{
  "name": "huddle-record",
  "version": "0.0.23",
  "description": "Record is a Object-Relational Mapping (ORM) TypeScript plugin that makes it easy to work with data.",
  "main": "lib/src/index.js",
  "types": "lib/src/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./src/index.ts",
    "example": "nodemon -I --exec node --experimental-specifier-resolution=node  --loader ts-node/esm ./examples/main/index.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huddlebv/record.git"
  },
  "keywords": [
    "Record",
    "ORM",
    "TypeScript"
  ],
  "author": "Jelle Scheer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/huddlebv/record/issues"
  },
  "homepage": "https://github.com/huddlebv/record#readme",
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-decorators": "*",
    "@babel/preset-env": "^7.18.2",
    "@types/node": "^18.11.7",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "axios": "^1.1.3"
  }
}
